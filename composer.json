{
    "name": "gbprod/elastica-provider-bundle",
    "description": "Bundle that can easely provide data in ElasticSearch using Elastica",
    "type": "bundle",
    "require": {
        "ruflin/elastica": "^3.2|^5.0|^6.0",
        "symfony/framework-bundle": "^2.7|^3.0|^4.0",
        "symfony/console": "^2.7|^3.0|^4.0",
        "symfony/event-dispatcher": "^2.7|^3.0|^4.0",
        "symfony/expression-language": "^2.7|^3.0|^4.0",
        "symfony/yaml": "^2.7|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "license": "WTFPL",
    "authors": [
        {
            "name": "GBProd",
            "email": "contact@gb-prod.fr"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "GBProd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\GBProd\\": "tests/"
        }
    },
    "suggest": {
        "gbprod/elastica-bundle": "Really simple bundle to use elastica within Symfony applications.",
        "gbprod/elastica-extra-bundle": "Bundle providing extra elastica tools for managing indices and types settings."
    }
}
